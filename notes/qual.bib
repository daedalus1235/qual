	pages = {228},
	file = {IOP Full Text PDF:/home/charles/Zotero/storage/33C3KQ9F/Satta et al. - 2015 - A QUANTUM STUDY OF THE CHEMICAL FORMATION OF CYANO.pdf:application/pdf},
