@article{haufler_absolute_1997,
	title = {Absolute {Integral} and {Differential} {Cross} {Sections} for the {Reactive} {Scattering} of {H}- + {D2} and {D}- + {H2}},
	volume = {101},
	issn = {1089-5639},
	url = {https://doi.org/10.1021/jp9707246},
	doi = {10.1021/jp9707246},
	abstract = {For the two reactions H- + D2 → D- + HD and D- + H2 → H- + HD, absolute integral and differential cross sections have been determined in a guided ion beam (GIB) apparatus. From the effective cross sections, measured in the translational energy range ET = 0.1−10 eV, thresholds for the onset of the reactions have been derived, indicating effective barrier heights of 350 ± 60 meV for H- + D2 and 330 ± 60 meV for D- + H2. These values are substantially lower than previously reported experimental and theoretical data. Comparison of the integral cross sections for the two isotopic variants reveals a significant isotope effect. The collision of the light ion with the heavy molecule leads to a cross section which is almost a factor of 2 smaller than for D- reacting with H2. Possible experimental effects discriminating D- versus H- are discussed. The recently reported differential cross section for H- + D2 which has been obtained with a crossed beam experiment is in good accordance with the present data determined with the GIB technique. A new result is that, at small collision energies, the angular dependence is very similar for both isotopic variants. In view of the quite large isotopic effect for the integral cross sections this is not self-evident. All these detailed results pose a challenge to full quantum dynamical scattering theory of the H3− collision system.},
	number = {36},
	urldate = {2024-01-13},
	journal = {J. Phys. Chem. A},
	author = {Haufler, E. and Schlemmer, S. and Gerlich, D.},
	month = sep,
	year = {1997},
	note = {Publisher: American Chemical Society},
	pages = {6441--6447},
	file = {Full Text PDF:/home/charles/Zotero/storage/2MKZA2XC/Haufler et al. - 1997 - Absolute Integral and Differential Cross Sections .pdf:application/pdf},
}

