	title = {A {QUANTUM} {STUDY} {OF} {THE} {CHEMICAL} {FORMATION} {OF} {CYANO} {ANIONS} {IN} {INNER} {CORES} {AND} {DIFFUSE} {REGIONS} {OF} {INTERS}℡{LAR} {MOLECULAR} {CLOUDS}},
